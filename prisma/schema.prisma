generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:password@127.0.0.1:3306/db"
}

model User {
  id       String  @id @unique()
  password String
  Token    Token[]
}

model Token {
  id           String @id @default(cuid())
  refreshToken String
  userId       String @unique
  user         User   @relation(fields: [userId], references: [id])
}
